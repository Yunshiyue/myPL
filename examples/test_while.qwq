int i = 1;
int ans = 0;
while (i <= 5) {
    i = i + 1;
}

ans = ans + 1;

while (i <= 10) {
    if (i == 8) {
        break;
    }
    i = i + 1;
}

if (i == 8) {
    ans = ans + 1;
}

int j = 1;
while (i <= 10) {
    if (j < i) {
        j = j + 1;
        continue;
    }
    break;
}

if (j == 8) {
    ans = ans + 1;
}