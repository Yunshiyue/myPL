array<array<array<int, 5>, 4>, 3> arr;

int num = 1;
for (int i = 0; i < 3; i = i + 1) {
    for (int j = 0; j < 4; j = j + 1) {
        for (int k = 0; k < 5; k = k + 1) {
            arr[i][j][k] = num;
            num = num + 1;
        }
    }
}

print("arr = ");
print(arr);
print("arr[1] = ");
print(arr[1]);
print("arr[1][2] = ");
print(arr[1][2]);
print("arr[1][2][3] = ");
print(arr[1][2][3]);